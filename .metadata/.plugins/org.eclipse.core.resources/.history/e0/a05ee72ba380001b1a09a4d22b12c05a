
public class School {
	public static void main(String[] args) {
		Teacher teacher = new Teacher("Sanjay");
		teacher.setAge((byte)24);
		System.out.println("Name is: "+teacher.getName());
		System.out.println("Age is: "+teacher.getAge());
	}
	
	void studentDiscussion() {
		System.out.println("Hello");
		Student student = new Student("Sanju");
	//	student.name = "ram";
		student.setAge((byte)24);
		System.out.println("Name is: "+student.getName());
		System.out.println("Age is: "+student.getAge());
